import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        int ans=4+((n-1)*3);
        System.out.print(ans);
	}
}
