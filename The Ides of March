import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        Scanner in=new Scanner(System.in);
        int d=in.nextInt();
        if(d==15)
        System.out.print("Yes");
        else
        System.out.print("No");
	}
}
